 uncanny ability to sniff out dycepskians occupying even the most unlikely of host bodies.<br/><br/>Site Owner: Blake DavisEmail Spam Checker: MX Guarddog</p>\n<p>&nbsp;</p>\n<section class=\"secret\">\n<h2>Ecology</h2>\n<h3>Environment</h3>\n<p>any</p>\n<p>&nbsp;</p>\n<h3>Organization</h3>\n<p>solitary</p>\n<p>&nbsp;</p>\n","public":""},"type":"Aberration","environment":"","cr":9,"xp":{"value":6400},"source":"AP #28 (p. 61)","raceAndGrafts":"","aura":""},"skills":{"acr":{"ability":"dex","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":false,"hasArmorCheckPenalty":true},"ath":{"ability":"str","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":false,"hasArmorCheckPenalty":true},"blu":{"ability":"cha","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":false,"hasArmorCheckPenalty":false},"com":{"ability":"int","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"cul":{"ability":"int","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"dip":{"ability":"cha","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":false,"hasArmorCheckPenalty":false},"dis":{"ability":"cha","ranks":0,"value":0,"misc":0,"mod":17,"enabled":true,"isTrainedOnly":false,"hasArmorCheckPenalty":false},"eng":{"ability":"int","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"int":{"ability":"cha","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":false,"hasArmorCheckPenalty":false},"lsc":{"ability":"int","ranks":0,"value":0,"misc":0,"mod":17,"enabled":true,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"med":{"ability":"int","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"mys":{"ability":"wis","ranks":0,"value":0,"misc":0,"mod":0,"enabled":false,"isTrainedOnly":true,"hasArmorCheckPenalty":false},"p