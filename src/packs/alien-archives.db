n":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":null,"units":null,"additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"","actionType":"mwak","attackBonus":10,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d6+5","slashing"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":null,"max":null},"usage":{"value":null,"per":""},"modifiers":[],"equippedBulkMultiplier":1,"acceptedItemTypes":{"weapon":false,"equipment":false,"goods":false,"consumable":false,"container":false,"technological":false,"fusion":true,"upgrade":false,"augmentation":false},"containedItemIds":[],"weaponType":"basicM","special":"","properties":{"analog":true,"one":true,"two":false,"amm":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"reach":false,"sniper":false,"stun":false,"thrown":false,"trip":false,"unwieldy":false,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"ignite":false,"indirect":false,"integrated":false,"living":false,"lockdown":false,"mind-affecting":false,"mine":false,"mire":false,"modal":false,"necrotic":false,"polarize":false,"polymorphic":false,"professional":false,"punchGun":false,"radioactive":false,"recall":false,"relic":false,"reposition":false,"shape":false,"shells":false,"shield":false,"subtle":false,"sunder":false,"swarm":false,"tail":false,"teleportive":false,"thought":false,"throttle":false,"underwater":false,"variantBoost":false,"wideLine":false},"proficient":true,"abilityMods":{"parts":[]},"container":{"contents":[],"storage":[{"type":"slot","subtype":"fusion","amount":2,"acceptsType":["fusion"],"affectsEncumbrance":true,"weightProperty":"level"}]}}},{"flags":{},"name":"Diasporan Rifle, Advanced","type":"weapon","img":"systems/sfrpg/icons/equipment/weapons/diasporan-rifle-advanced.jpg","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListTalentsAll_ctl00_LabelName\">These sleek firearms were developed by the finest sarcesian engineers for use by elite mercenaries. The most common manufacturer is MuniTech, one of the only corporations fully owned and operated by sarcesians. Diasporan rifles are sniper rifles that belong to the laser category.</span></p>\n<h3 class=\"framing\"><strong>Laser Weapons</strong></h3>\n<hr>\n<p>Laser weapons emit highly focused beams of light that deal fire damage. These beams can pass through glass and other transparent physical barriers, dealing damage to such barriers as they pass through. Barriers of energy or magical force block lasers. Invisible creatures don’t take damage from lasers, as the beams pass through them harmlessly. Fog, smoke, and other clouds provide both cover and concealment from laser attacks. Lasers can penetrate darkness, but they don’t provide any illumination.</p>\n<p>Laser weapons use various means to concentrate beams of light into deadly intensity. Some focus light through a faceted crystal, while others focus the beam through a chemical cloud or ionized gas.</p>","chat":"","unidentified":""},"source":"Alien Archive","type":"","quantity":1,"bulk":"2","price":3750,"level":5,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effec